{
  "name": "bounty_mode",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "scriptedeuch": "github:Orb-Workshop/scriptedeuch"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "bun build ./src/main.ts --outfile ./scripts/bounty_mode.js --external cs_script/point_script",
    "watch": "bun --watch build ./src/main.ts --outfile ./scripts/bounty_mode.js --external cs_script/point_script",
    "postinstall": "bun run ./bun_postinstall.ts"
  }
}
